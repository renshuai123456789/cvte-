<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cvte.dao.UserMapper" >
  <insert id="insertUser" parameterType="cvte.vo.CvteUser">
    insert into cvte_user (username,realname,pwd,phone,power,delete1) values(#{username},#{realname},#{pwd},#{phone},#{power},#{delete1});
  </insert>

  <select id="selectOne" parameterType="int" resultType="cvte.vo.CvteUser" >
    select *from cvte_user where id=#{id};
  </select>

  <select id="selectByName" resultType="cvte.vo.CvteUser">
    select *from cvte_user where username=#{name} and pwd=#{pwd};
  </select>


</mapper>